OBJECT=VAR_1
VAR_1:TYPE=INTEGER
VAR_1:VALUE=0
VAR_1:ONCHANGED^2=SHOWPROMIEN

OBJECT=UFOLECI
UFOLECI:TYPE=ANIMO
UFOLECI:VISIBLE=TRUE
UFOLECI:FILENAME=UFO.ANN
UFOLECI:TOCANVAS=TRUE
UFOLECI:PRIORITY=20
UFOLECI:FPS=16
UFOLECI:PRELOAD=TRUE
UFOLECI:RELEASE=TRUE
UFOLECI:MONITORCOLLISION=FALSE
UFOLECI:MONITORCOLLISIONALPHA=FALSE
UFOLECI:ONINIT={THIS^PLAY("1");VARINTRO^SET(1);}
UFOLECI:ONFINISHED^1={THIS^PLAY("2"):;}
