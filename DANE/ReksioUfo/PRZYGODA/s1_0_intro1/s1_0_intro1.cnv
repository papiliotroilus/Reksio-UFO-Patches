OBJECT=MIASTO
MIASTO:TYPE=IMAGE
MIASTO:VISIBLE=TRUE
MIASTO:FILENAME=MIASTO2.IMG
MIASTO:TOCANVAS=TRUE
MIASTO:PRIORITY=1
MIASTO:PRELOAD=TRUE
MIASTO:RELEASE=TRUE
MIASTO:MONITORCOLLISION=FALSE
MIASTO:MONITORCOLLISIONALPHA=FALSE
MIASTO:ONINIT=THISHIDE

OBJECT=TIMER1
TIMER1:TYPE=TIMER
TIMER1:ENABLED=TRUE
TIMER1:ELAPSE=100
TIMER1:TICKS=0
TIMER1:ONINIT={THIS^DISABLE():;}
TIMER1:ONTICK=COTICK
TIMER1:ONTICK^110=UFOLADUJE
TIMER1:ONTICK^1=USTAWMIASTO
TIMER1:ONTICK^155=STOPTIMER1

OBJECT=TIMERKUR
TIMERKUR:TYPE=TIMER
TIMERKUR:ENABLED=TRUE
TIMERKUR:ELAPSE=100
TIMERKUR:TICKS=0
TIMERKUR:ONINIT={THIS^DISABLE():;}
TIMERKUR:ONTICK^12=BEHKURA1
TIMERKUR:ONTICK^20=BEHKURA2
TIMERKUR:ONTICK^28=BEHKURA3
TIMERKUR:ONTICK^36=BEHKURA4
TIMERKUR:ONTICK^57=BEHKURA1
TIMERKUR:ONTICK^64=BEHKURA2
TIMERKUR:ONTICK^72=BEHKURA3
TIMERKUR:ONTICK^80=BEHKURA4
TIMERKUR:ONTICK^97=BEHKURA1
TIMERKUR:ONTICK^105=BEHKURA2
TIMERKUR:ONTICK^112=BEHKURA3
TIMERKUR:ONTICK^120=BEHKURA4
TIMERKUR:ONTICK^160=UFOODLATUJE
TIMERKUR:ONTICK^205=BEHKURATOR
TIMERKUR:ONTICK^247=PRZELADOWANIE
TIMERKUR:ONTICK^289=DOINTRO2

OBJECT=MOUSE
MOUSE:TYPE=MOUSE
MOUSE:ONCLICK={DOINTRO2^RUN();}

OBJECT=CNVLOADER
CNVLOADER:TYPE=CNVLOADER

OBJECT=CANVASOBSERVER
CANVASOBSERVER:TYPE=CANVAS_OBSERVER

OBJECT=COND1
COND1:TYPE=CONDITION
COND1:OPERAND1=MIASTO^GETPOSITIONY()
COND1:OPERATOR=LESS
COND1:OPERAND2=0


OBJECT=KEYBOARD
KEYBOARD:TYPE=KEYBOARD

OBJECT=USTAWMIASTO
USTAWMIASTO:TYPE=BEHAVIOUR
USTAWMIASTO:CODE={MIASTO^SETPOSITION(0,600);MIASTO^SHOW();}


OBJECT=STARTRUCHU
STARTRUCHU:TYPE=BEHAVIOUR
STARTRUCHU:CODE={UFOLECI^PLAY("3");TIMER1^ENABLE();LOGO^PLAY("PLAY");}


OBJECT=COTICK
COTICK:TYPE=BEHAVIOUR
COTICK:CODE={COND1^ONE_BREAK(TRUE);MIASTO^MOVE(0,-4);GWIAZDY^MOVE(0,-3);}


OBJECT=UFOLADUJE
UFOLADUJE:TYPE=BEHAVIOUR
UFOLADUJE:CODE={UFOLECI^PLAY("4");LOGO^PLAY("DOWN");}


OBJECT=SHOWPROMIEN
SHOWPROMIEN:TYPE=BEHAVIOUR
SHOWPROMIEN:CODE={PROMIEN^PLAY("PLAY");}


OBJECT=STOPTIMER1
STOPTIMER1:TYPE=BEHAVIOUR
STOPTIMER1:CODE={TIMER1^DISABLE();VAR_1^INC();}


OBJECT=THISHIDE
THISHIDE:TYPE=BEHAVIOUR
THISHIDE:CODE={THIS^HIDE();}


OBJECT=STARTKUR
STARTKUR:TYPE=BEHAVIOUR
STARTKUR:CODE={TIMER1^DISABLE();TIMERKUR^ENABLE();}


OBJECT=BEHKURA1
BEHKURA1:TYPE=BEHAVIOUR
BEHKURA1:CODE={KURA1^PLAY("PLAY");}


OBJECT=BEHKURA2
BEHKURA2:TYPE=BEHAVIOUR
BEHKURA2:CODE={KURA2^PLAY("PLAY");}


OBJECT=BEHKURA3
BEHKURA3:TYPE=BEHAVIOUR
BEHKURA3:CODE={KURA3^PLAY("PLAY");}


OBJECT=BEHKURA4
BEHKURA4:TYPE=BEHAVIOUR
BEHKURA4:CODE={UFOKURA^PLAY("PLAY");}


OBJECT=BEHKURATOR
BEHKURATOR:TYPE=BEHAVIOUR
BEHKURATOR:CODE={KURATOR^PLAY("PLAY");}


OBJECT=UFOODLATUJE
UFOODLATUJE:TYPE=BEHAVIOUR
UFOODLATUJE:CODE={PROMIEN^HIDE();UFOLECI^PLAY("ODLATUJE");}


OBJECT=PRZELADOWANIE
PRZELADOWANIE:TYPE=BEHAVIOUR
PRZELADOWANIE:CODE={MIASTO^HIDE();KURATOR^HIDE();CANVASOBSERVER^SETBACKGROUND("BKG2.IMG");CANVASOBSERVER^REFRESH();OKO^PLAY("PLAY");}


OBJECT=DOINTRO2
DOINTRO2:TYPE=BEHAVIOUR
DOINTRO2:CODE={PRZYGODA^GOTO("S2_0_INTRO2");}


OBJECT=POKAZLOGO
POKAZLOGO:TYPE=BEHAVIOUR
POKAZLOGO:CODE={LOGO^PLAY("PLAY");}


OBJECT=BEHVAR
BEHVAR:TYPE=BEHAVIOUR
BEHVAR:CODE={VAR_1^INC();}




